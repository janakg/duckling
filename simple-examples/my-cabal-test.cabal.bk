-- Initial my-cabal-test.cabal generated by cabal init.  For further 
-- documentation, see http://haskell.org/cabal/users-guide/

name:                my-cabal-test
version:             0.1.0.0
-- synopsis:            
-- description:         
license:             BSD3
license-file:        LICENSE
author:              Santhiya Viswanathan
maintainer:          mailtosanthiya.v@gmail.com
-- copyright:           
-- category:            
build-type:          Simple
extra-source-files:  ChangeLog.md
cabal-version:       >=1.10

executable my-cabal-test
  main-is:             Main.hs
  -- other-modules:       
  -- other-extensions:    
  build-depends:       base

  -- hs-source-dirs:      
  default-language:    Haskell2010

executable fib-test
  main-is:             fib.hs
  -- other-modules:       
  -- other-extensions:    
  build-depends:       base
                    --  , asterius-prelude
  ghc-options:        -Wall

  -- hs-source-dirs:      
  default-language:    Haskell2010

executable string-test
  main-is:             str.hs
  -- other-modules:       
  -- other-extensions:    
  build-depends:       base
                     , text
                     , bytestring
                     , asterius-prelude 
                     , text-conversions
                     , jsaddle

  -- hs-source-dirs:      
  default-language:    Haskell2010
  default-extensions:  OverloadedStrings

